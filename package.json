{
  "name": "booking-manager",
  "version": "1.0.0",
  "description": "Booking Manager",
  "main": "dist/server/server.js",
  "scripts": {
    "clean": "rm -rf ./dist ./static/bundle.js ./static/*.gz ./static/*.min.css",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx src",
    "test": "nodemon src/server/server.js --exec babel-node",
    "build-dev": "babel src -d dist; webpack --config webpack.config.js",
    "build": "babel src -d dist; cleancss -02 -o ./static/style.min.css ./static/style.css; cleancss -02 -o ./static/style-nojs.min.css ./static/style-nojs.css; webpack -p --config webpack.config.prod.js",
    "start-dev": "node dist/server/server.js",
    "start": "NODE_ENV='production' node dist/server/server.js"
  },
  "author": "Netflux",
  "repository": {
    "type": "git",
    "url": "https://github.com/Netflux/booking-manager.git"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "connect-mongodb-session": "^1.3.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "material-ui": "^0.17.0",
    "moment": "^2.17.1",
    "mongoose": "^4.8.6",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "clean-css-cli": "^4.0.10",
    "compression-webpack-plugin": "^0.3.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1"
  }
}
